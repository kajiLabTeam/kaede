{
  "name": "kaede",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@hono/eslint-config": "^2.0.6",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "hono": "^4.12.3",
    "prettier": "^3.8.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260305.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "wrangler": "^4.4.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=22.13.0",
    "pnpm": ">=10.0.0"
  }
}
